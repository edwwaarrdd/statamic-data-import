    "require": {
        "statamic/cms": "^3.0|^4.0",
        "spatie/simple-excel": "^1.9|^2.0|^3.0",
        "ext-mbstring": "*"
    },
    "extra": {
        "statamic": {
            "name": "Statamic Data Import",
            "description": "Statamic Data Import addon"
        },
        "laravel": {
            "providers": [
                "Rias\\StatamicDataImport\\DataImportServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
